{
  "name": "eav-to-prisma-example-blog",
  "version": "1.0.0",
  "private": true,
  "description": "E2E example with real Prisma client",
  "scripts": {
    "setup": "yarn prisma:generate && yarn prisma:migrate && yarn seed",
    "prisma:generate": "prisma generate --schema=prisma/eav-schema.prisma",
    "prisma:migrate": "prisma migrate deploy --schema=prisma/eav-schema.prisma",
    "prisma:migrate:dev": "prisma migrate dev --schema=prisma/eav-schema.prisma",
    "prisma:migrate:reset": "prisma migrate reset --schema=prisma/eav-schema.prisma --force",
    "prisma:studio": "prisma studio --schema=prisma/eav-schema.prisma",
    "prisma:format": "prisma format --schema=prisma/eav-schema.prisma",
    "seed": "tsx prisma/seed.ts",
    "generate": "tsx ../../src/cli/index.ts generate",
    "test": "vitest run",
    "test:watch": "vitest",
    "clean": "rm -rf prisma/eav.db prisma/generated-content.prisma node_modules/.prisma"
  },
  "dependencies": {
    "@prisma/client": "^6.0.0"
  },
  "devDependencies": {
    "prisma": "^6.0.0",
    "tsx": "^4.20.6",
    "vitest": "^4.0.14"
  }
}
