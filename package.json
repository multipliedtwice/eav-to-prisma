{
  "name": "eav-to-prisma",
  "version": "0.0.0",
  "description": "Generate Prisma schemas from EAV database structures",
  "author": "multipliedtwice",
  "license": "MIT",
  "keywords": [
    "prisma",
    "prisma-generator",
    "eav",
    "entity-attribute-value",
    "code-generator",
    "cms",
    "headless-cms",
    "schema-generator",
    "typescript",
    "i18n",
    "database",
    "orm"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": "./dist/cli/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/multipliedtwice/eav-to-prisma.git"
  },
  "bugs": {
    "url": "https://github.com/multipliedtwice/eav-to-prisma/issues"
  },
  "homepage": "https://github.com/multipliedtwice/eav-to-prisma#readme",
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/multipliedtwice"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "type-check": "tsc --noEmit",
    "lint": "eslint src",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepublishOnly": "yarn build && yarn test:run",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:run": "vitest run",
    "prepack": "yarn build"
  },
  "dependencies": {
    "chalk": "^5.6.2",
    "commander": "^14.0.2",
    "dedent": "^1.7.0",
    "dotenv": "^17.2.3",
    "ora": "^9.0.0",
    "prompts": "^2.4.2",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/node": "^24.10.1",
    "@types/prompts": "^2.4.9",
    "@vitest/coverage-v8": "^4.0.14",
    "eslint": "^9.39.1",
    "prettier": "^3.6.2",
    "prettier-plugin-prisma": "^5.0.0",
    "semantic-release": "^25.0.2",
    "tsup": "^8.5.1",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "^4.0.14"
  },
  "peerDependencies": {
    "@prisma/client": "^6.0.0",
    "prisma": "^6.0.0"
  },
  "peerDependenciesMeta": {
    "prisma": {
      "optional": false
    }
  },
  "engines": {
    "node": ">=22.14.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
